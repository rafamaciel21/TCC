    <sql>-- Stored procedure para retornar custo do produto
CREATE OR REPLACE PROCEDURE CONV.SP_GET_CUSTO(
												IN 	LI_IDEMPRESA		INTEGER,
												IN 	LI_IDPRODUTO		INTEGER,
												IN 	LI_IDSUBPRODUTO		INTEGER,
												OUT LD_CUSTO 			DECIMAL(31,7)
											 )
LANGUAGE SQL											 
BEGIN	
	SET LD_CUSTO = (SELECT 
					    CASE
					        WHEN CUSTOGERENCIAL     > 0 THEN
					            CUSTOGERENCIAL
					        WHEN CUSTOULTIMACOMPRA  > 0 THEN
					            CUSTOULTIMACOMPRA
					        WHEN CUSTONOTAFISCAL    > 0 THEN
					            CUSTONOTAFISCAL
					        WHEN VALCUSTOREPOS      > 0 THEN
					            VALCUSTOREPOS
					    ELSE
					        0
					    END
					FROM 
						DBA.POLITICA_PRECO_PRODUTO
					WHERE
						IDEMPRESA 			= LI_IDEMPRESA		AND 
						IDPRODUTO			= LI_IDPRODUTO 		AND
						IDSUBPRODUTO		= LI_IDSUBPRODUTO	
						FETCH FIRST 1 ROWS ONLY
					);
END</sql>
