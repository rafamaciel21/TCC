    <quoteString>N</quoteString>
    <sql>-- Stored procedure para criar o lote quando o mesmo não existe
CREATE OR REPLACE PROCEDURE CONV.SP_CRIA_LOTE(
												IN 	LI_IDPRODUTO		INTEGER,
												IN 	LI_IDSUBPRODUTO		INTEGER
											 )
LANGUAGE SQL											 
BEGIN	
		DECLARE LV_LOTE VARCHAR(20);
		SET LV_LOTE = 'GERAL';	
	
		INSERT INTO DBA.PRODUTO_GRADE_LOTE(IDLOTE,IDPRODUTO,IDSUBPRODUTO)
		SELECT 
			LV_LOTE,LI_IDPRODUTO,LI_IDSUBPRODUTO
		FROM 
			DUMMY
		WHERE NOT EXISTS (SELECT 1 FROM DBA.PRODUTO_GRADE_LOTE WHERE IDPRODUTO = LI_IDPRODUTO AND IDSUBPRODUTO = LI_IDSUBPRODUTO AND IDLOTE = LV_LOTE);
END;
COMMIT
</sql>
