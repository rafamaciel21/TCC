      <note>-- Quando necessário usar a tabela com de/para de códigos (Condição CCO)


-- Exportar códigos de produto do banco ASA e importar no DB2
SELECT
	Codigo_Produto 		AS Codigo_Produto,
	Codigo_Produto 		AS Codigo_Produto_Derivado,
	NOVOIDPRODUTO 		AS IDPRODUTO,
	NOVOIDSUBPRODUTO 	AS IDSUBPRODUTO
FROM 
	CONV.V1_PRODUTOS 
WHERE 
	FLAGCONVER =  'T'  ; OUTPUT TO C:\TMP\CONVERSAO_PRODUTO.TXT


DB2 IMPORT FROM C:\TMP\CONVERSAO_PRODUTO.TXT OF DEL MODIFIED BY CHARDEL'''' INSERT INTO CONV.CONVERSAO_PRODUTO(Codigo_Produto,Codigo_Produto_Derivado,IDPRODUTO,IDSUBPRODUTO)


-- Exportar códigos de clientes e fornecedores do banco ASA e importar no DB2
SELECT
        CODIGO_CLIENTE_FORNECEDOR       as CODIGO_CLIENTE_FORNECEDOR,
        NOVOIDCLIFOR                    AS IDCLIFOR
FROM
        CONV.V1_CLIENTES_FORNECEDORES
WHERE
        FLAGCONVER IN ('T','J','D') ; OUTPUT TO C:\TMP\CONVERSAO_CLIENTE_FORNECEDOR.TXT

