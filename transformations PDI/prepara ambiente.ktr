    <sql>-- Stored procedure para verificar se o produto tem lote.
CREATE OR REPLACE PROCEDURE CONV.SP_VERIFICA_LOTE(
												IN 	LI_IDPRODUTO		INTEGER,
												IN 	LI_IDSUBPRODUTO		INTEGER,
												OUT LV_LOTE 			CHAR(1)
											 )
LANGUAGE SQL											 
BEGIN	
	SET LV_LOTE  = 
					(SELECT
        				CASE
                			WHEN EXISTS (SELECT 1 FROM DBA.PRODUTO_GRADE_LOTE WHERE IDPRODUTO = LI_IDPRODUTO AND IDSUBPRODUTO = LI_IDSUBPRODUTO )  THEN 'S'
                			ELSE 'N'
        				END
					FROM
        				DBA.DUMMY);
END;
COMMIT</sql>
