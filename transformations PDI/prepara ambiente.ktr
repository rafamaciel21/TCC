    <sql>-- Sequence responsável por gerar o IDADIANTAMENTO para as rotinas de conversão dos adiantamentos
BEGIN
        DECLARE LS_STMT       	VARCHAR(500);        
        SET 	LS_STMT       	= 'CREATE SEQUENCE CONV.SEQ_IDADIANTAMENTO_CONV START WITH 1 INCREMENT BY 1';
       
        IF ( SELECT 1 FROM SYSCAT.SEQUENCES WHERE SEQNAME = 'SEQ_IDADIANTAMENTO_CONV' ) IS NULL THEN 
	        EXECUTE IMMEDIATE LS_STMT;     
		END IF; 
END</sql>
